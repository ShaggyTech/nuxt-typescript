{
  "restartable": "rs",
  "ignore": [".git", "node_modules/**/node_modules"],
  "verbose": false,
  "execMap": {
    "ts": "node --require ts-node/register"
  },
  "watch": [
    "assets/",
    "components/",
    "compositions/",
    "layouts/",
    "middleware/",
    "pages/",
    "plugins/",
    "server/",
    "static/",
    "store/",
    "types/",
    "utils/"
  ],
  "env": {
    "NODE_ENV": "development",
    "TS_NODE_PROJECT": "server/tsconfig.json"
  },
  "ext": "js,mjs,json,ts,vue"
}