{
  "name": "example-composition-api-advanced",
  "description": "An advanced example app for Nuxt TypeScript using the Vue Composition API",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node --project server/tsconfig.json server/server-dev.ts",
    "build": "nuxt-ts build",
    "start": "cross-env NODE_ENV=production ts-node --project 'server/tsconfig.json' server/server-prod.ts",
    "generate": "nuxt-ts generate",
    "post-update": "yarn upgrade --latest"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "latest",
    "@vue/composition-api": "latest",
    "cross-env": "latest",
    "nuxt-edge": "latest"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "latest",
    "express": "latest"
  }
}
